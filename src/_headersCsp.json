{
  "base-uri": "'none'",
  "default-src": ["'self'", "data:", "https:", "wss:"],
  "style-src": ["'self'", "data:", "https:", "wss:", "'unsafe-inline'"],
  "script-src": [
    "'self'",
    "https://api.scrivito.com",
    "https://assets.scrivito.com"
  ],
  "object-src": "'none'",
  "block-all-mixed-content": true,
  "frame-ancestors": ["'self'", "https://*.scrivito.com"]
}
